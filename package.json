{
  "name": "blog-content-generator",
  "version": "1.0.0",
  "description": "GPT-powered blog content generator with Unsplash integration",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node server/index.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "keywords": ["gpt", "blog", "content-generator", "unsplash"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "openai": "^4.20.1",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}

